    "@babel/preset-env": "^7.1.0",
    "@types/node": "^12.0.4",
    "@types/superagent": "^4.1.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "base64-inline-loader": "^1.1.1",
    "cache-loader": "^2.0.1",
