{
  "name": "teeq-lab",
  "version": "1.0.0",
  "description": "Teeq Sandbox",
  "main": "src/bootstrap.ts",
  "author": "Teeqer",
  "license": "MIT",
  "private": true,
  "scripts": {
    "swagger:post-handling": "DIRECTORY=./src/app/api node ./node_modules/teeqer-core/swagger/removeUnwantedFiles.js",
    "swagger:codegen": "swagger-codegen generate -i ./src/app/swagger.json -l typescript-fetch -t ./node_modules/teeqer-core/swagger/template -o ./src/app/api",
    "swagger:generate": "yarn swagger:codegen && yarn swagger:post-handling",
    "dev": "webpack-serve --config ./conf/webpack/dev.config.js",
    "build": "webpack --config ./conf/webpack/prod.config.js",
    "build:item": "webpack --config ./conf/webpack/prod__item.config.js",
    "deploy": "firebase deploy",
    "update:core": "rm -rf node_modules/core && yarn upgrade core",
    "update:uxui": "rm -rf node_modules/teeqer-uxui && yarn upgrade teeqer-uxui",
    "update": "rm -rf node_modules/teeqer-uxui && rm -rf node_modules/teeqer-core && yarn upgrade teeqer-uxui teeqer-core"
  },
  "dependencies": {
    "@microsoft/tsdoc": "^0.12.8",
