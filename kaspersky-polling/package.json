    "lit-element": "^2.0.0-rc.5",
    "sort-array": "^2.0.0",
    "superagent": "^5.0.5",
    "swagger-client": "^3.8.25",
    "teeqer-core": "git+https://github.com/teeqer/core#lit-1.2.9.2",
    "teeqer-uxui": "git+https://github.com/teeqer/teeqer-uxui#lit-element",
    "uid-generator": "^1.0.0"
